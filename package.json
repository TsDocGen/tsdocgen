{
  "name": "@tsdocgen/core",
  "version": "0.0.39",
  "description": "Documentation generator for typescript",
  "main": "index.js",
  "types": "index.d.js",
  "private": false,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf dist",
    "exec": "ts-node ./scripts/tsdocgen",
    "build": "tsc -p tsconfig.build.json",
    "copyPackageJson": "cp -rf package.json dist",
    "dist": "yarn run clean && yarn run build && yarn run copyPackageJson",
    "release": "yarn run dist && cd dist && yarn publish --access public",
    "test-release": "yarn run dist && cd dist && npm publish --dry-run"
  },
  "author": "Zerry Hogan",
  "license": "ISC",
  "devDependencies": {
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@tsdocgen/core": "^0.0.24",
    "@tsdocgen/themes": "^0.0.12",
    "@types/bunyan": "^1.8.8",
    "@types/glob": "^7.1.4",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.173",
    "@types/node": "^16.9.1",
    "@types/react-helmet": "^6.1.2",
    "jest": "^27.4.5",
    "reflect-metadata": "^0.1.13"
  },
  "dependencies": {
    "@schemastore/package": "^0.0.6",
    "@types/react": "^17.0.21",
    "@types/react-dom": "^17.0.9",
    "ansi-colors": "^4.1.1",
    "bunyan": "^1.8.15",
    "chalk": "^5.0.0",
    "comment-parser": "^1.2.4",
    "cosmiconfig": "^7.0.1",
    "flatted": "^3.2.2",
    "glob": "^7.1.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "ts-morph": "^12.0.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  }
}
